#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/juliesimac/dev/holly-v6/apps/frontend/node_modules/.pnpm/shadcn-ui@0.6.0/node_modules/shadcn-ui/dist/node_modules:/Users/juliesimac/dev/holly-v6/apps/frontend/node_modules/.pnpm/shadcn-ui@0.6.0/node_modules/shadcn-ui/node_modules:/Users/juliesimac/dev/holly-v6/apps/frontend/node_modules/.pnpm/shadcn-ui@0.6.0/node_modules:/Users/juliesimac/dev/holly-v6/apps/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/juliesimac/dev/holly-v6/apps/frontend/node_modules/.pnpm/shadcn-ui@0.6.0/node_modules/shadcn-ui/dist/node_modules:/Users/juliesimac/dev/holly-v6/apps/frontend/node_modules/.pnpm/shadcn-ui@0.6.0/node_modules/shadcn-ui/node_modules:/Users/juliesimac/dev/holly-v6/apps/frontend/node_modules/.pnpm/shadcn-ui@0.6.0/node_modules:/Users/juliesimac/dev/holly-v6/apps/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/index.js" "$@"
else
  exec node  "$basedir/../../dist/index.js" "$@"
fi
